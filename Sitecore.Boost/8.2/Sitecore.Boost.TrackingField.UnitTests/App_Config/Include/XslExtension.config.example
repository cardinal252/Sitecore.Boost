<!--

Purpose: This include file shows how you can add custom XslExtensions

To enable this, rename this file so that it has a ".config" extension and 
replace the "type" and "namespace" attributes below with your own values

-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <xslExtensions>
      <extension mode="on" type="Demo.Samples.XslHelper, MyDemo" namespace="http://www.sitecore.net/demoXslHelper" singleInstance="true"/>
    </xslExtensions>
  </sitecore>
</configuration>